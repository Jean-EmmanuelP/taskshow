{
  "name": "taskflow-project",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "TaskFlow MERN Stack Task Management Application",
  "keywords": ["taskflow", "mern", "task-management", "react", "nodejs", "mongodb"],
  "author": "TaskFlow Team",
  "license": "MIT",
  "scripts": {
    "setup": "./setup.sh",
    "setup:windows": "setup.bat",
    "dev": "vite",
    "dev:all": "./start-all.sh",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm run dev",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cd server && npm run dev",
    "start:server": "cd server && npm start",
    "install:server": "cd server && npm install",
    "install:all": "npm install && npm run install:server",
    "clean": "rm -rf node_modules server/node_modules .env.local server/.env",
    "clean:windows": "rmdir /s /q node_modules server\\node_modules && del .env.local server\\.env",
    "test": "echo \"No tests configured yet\" && exit 0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-big-calendar": "^1.19.4",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.17.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/taskflow.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/taskflow/issues"
  },
  "homepage": "https://github.com/your-username/taskflow#readme"
}
